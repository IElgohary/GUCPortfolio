<% include _header %>
<h1>Are you sure you want to delete this project from your portfolio?</h1>
<form action="/delete" method="post">
    <input type="hidden" name="id" value="<%= project._id %>">
    <input type="submit" value="Confirm" class="btn btn-danger ">
    <a href="/portfolio/<%= project._creator.username%>" class="btn btn-primary">Cancel</a>
</form>

<% include _footer %>